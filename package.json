{
  "name": "zsebtanar-firebase",
  "version": "1.0.0",
  "keywords": [
    "teaching",
    "mathematics",
    "students",
    "open-source",
    "non-profit",
    "hungarian"
  ],
  "description": "Zsebtanár egy matematika oktatást segítő interaktív weboldal magyar diákok számára",
  "homepage": "https://zsebtanar.hu",
  "bugs": "https://github.com/zsebtanar/zsebtanar-proto/issues",
  "private": true,
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "start": "npm run ::client-dev",
    "build": "cross-env NODE_ENV=production npm run ::server-build && npm run ::client-build",
    "predeploy": "npm run test",
    "deploy": "npm run build && firebase --project=test deploy",
    "postbuild": "node build/post-build",
    "start:server": "npm run ::server-build && firebase serve --only functions",
    "deploy:server": "npm run ::server-build && ./node_modules/.bin/firebase --project=test deploy --only functions",
    "server:shell": "npm run build && firebase functions:shell",
    "server:logs": "firebase functions:log",
    "check-types": "tsc --no-emit",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "concurrently --kill-others --names \"test,type-check,format-check,lint,build\\ \"npm run test\" \"npm run check-types\" \"npm run check-format\" \"npm run lint\" \"npm run build\"",
    "precommit": "lint-staged",
    "::client-dev": "webpack-dev-server --config ./build/webpack.client.js",
    "::client-build": "cross-env NODE_ENV=production webpack -p --config ./build/webpack.client.js",
    "::server-build": "npm run ::server-compile",
    "::server-dev": "concurrently --kill-others --names \"server,firebase,client\" \"SERVER_ENV=emulator npm run ::server-compile -- --watch\" \"npm run ::server-emulator\" \"SERVER_ENV=emulator npm run ::client-dev\"",
    "pre::server-compile": "npm run ::server-prep",
    "::server-compile": "cross-env NODE_ENV=production webpack --display-error-details --config ./build/webpack.server.js",
    "::server-emulator": "firebase use test && firebase functions:config:get > bin/functions/.runtimeconfig.json && cross-env GOOGLE_APPLICATION_CREDENTIALS=\"$(pwd)/tools/key.json\" firebase emulators:start --only functions",
    "::server-prep": "node build/prepare-server"
  },
  "config": {
    "firebaseFunctionDepencies": [
      "algoliasearch",
      "cookie-parser",
      "cors",
      "body-parser",
      "express",
      "firebase-admin",
      "firebase-functions",
      "joi",
      "markdown-it",
      "markdown-it-katex",
      "markdown-it-kbd",
      "markdown-it-center-text",
      "ramda"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@sentry/browser": "^5.10.2",
    "@sentry/types": "^5.10.0",
    "@types/react-redux": "^7.1.5",
    "@types/react-router": "^5.1.3",
    "algoliasearch": "3.35.1",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "codemirror": "^5.53.2",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "dot-prop-immutable": "^2.1.0",
    "downshift": "^5.4.2",
    "express": "4.17.1",
    "firebase": "^7.14.4",
    "firebase-admin": "^8.12.1",
    "firebase-functions": "^3.6.1",
    "history": "4.10.1",
    "joi": "14.3.1",
    "markdown-it": "10.0.0",
    "markdown-it-center-text": "^1.0.4",
    "markdown-it-katex": "git+https://git@github.com/maxinteger/markdown-it-katex.git#v2.2.0",
    "markdown-it-kbd": "^2.0.0",
    "pocket-lisp": "latest",
    "pocket-lisp-stdlib": "latest",
    "ramda": "0.26.1",
    "react": "16.13.1",
    "react-async-script": "^1.1.1",
    "react-dnd": "10.0.2",
    "react-dnd-html5-backend": "10.0.2",
    "react-dom": "16.12.0",
    "react-ga": "2.7.0",
    "react-google-recaptcha": "^2.0.1",
    "react-input-autosize": "2.2.2",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-select": "3.0.8",
    "react-test-renderer": "^16.12.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "zod": "^1.6.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/algoliasearch": "^3.34.7",
    "@types/codemirror": "0.0.91",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.9.1",
    "@types/joi": "^14.3.4",
    "@types/node": "^12.12.17",
    "@types/ramda": "^0.26.38",
    "@types/raven": "2.5.3",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-redux": "^7.1.8",
    "@types/react-router": "^5.1.7",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "awesome-typescript-loader": "5.2.0",
    "bootstrap": "^4.4.1",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "css-loader": "3.3.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-jest": "^3.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "firebase-tools": "8.2.0",
    "html-webpack-harddisk-plugin": "1.0.1",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.1.0",
    "inline-chunk-manifest-html-webpack-plugin": "^2.0.0",
    "jest": "^26.0.0",
    "lint-staged": "9.5.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "4.13.0",
    "prettier": "1.19.1",
    "react-axe": "^3.3.0",
    "react-hot-ts": "^0.7.3",
    "sass-loader": "8.0.0",
    "shelljs": "0.8.3",
    "source-map-loader": "0.2.4",
    "style-loader": "1.0.1",
    "ts-jest": "^25.5.1",
    "ts-loader": "^6.2.1",
    "typescript": "3.9.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
