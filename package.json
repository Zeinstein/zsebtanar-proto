    "@types/react-router": "^5.1.3",
    "algoliasearch": "3.35.1",
    "axios": "0.19.0",
    "classnames": "^2.2.6",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "dot-prop-immutable": "^2.1.0",
    "express": "4.17.1",
    "firebase": "7.6.0",
    "firebase-admin": "8.8.0",
    "firebase-functions": "3.3.0",
    "history": "4.10.1",
    "joi": "14.3.1",
    "markdown-it": "10.0.0",
    "markdown-it-center-text": "^1.0.4",
    "markdown-it-katex": "git+https://git@github.com/maxinteger/markdown-it-katex.git#v2.2.0",
    "markdown-it-kbd": "^2.0.0",
    "ramda": "0.26.1",
    "react": "16.12.0",
    "react-async-script": "^1.1.1",
    "react-dnd": "10.0.2",
    "react-dnd-html5-backend": "10.0.2",
    "react-dom": "16.12.0",
    "react-ga": "2.7.0",
    "react-google-recaptcha": "^2.0.1",
    "react-input-autosize": "2.2.2",
    "react-redux": "7.1.3",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-select": "3.0.8",
    "react-test-renderer": "^16.12.0",
    "redux": "4.0.4",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "xstate": "latest",
    "@xstate/react": "latest"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/algoliasearch": "^3.34.7",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.23",
    "@types/joi": "^14.3.4",
    "@types/node": "^12.12.17",
    "@types/ramda": "^0.26.38",
    "@types/raven": "2.5.3",
    "@types/react": "16.9.16",
    "@types/react-dom": "16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "bootstrap": "4.4.1",
    "css-loader": "3.3.2",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-import-resolver-jest": "^3.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "firebase-tools": "7.10.0",
    "html-webpack-harddisk-plugin": "1.0.1",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.1.0",
    "inline-chunk-manifest-html-webpack-plugin": "^2.0.0",
    "jest": "^24.9.0",
    "lint-staged": "9.5.0",
    "node-sass": "4.13.0",
    "npm-run-all": "^4.1.5",
    "prettier": "1.19.1",
    "react-axe": "^3.3.0",
    "sass-loader": "8.0.0",
    "shelljs": "0.8.3",
    "source-map-loader": "0.2.4",
    "style-loader": "1.0.1",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "typescript": "3.7.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "start": "npm run ::client-dev",
    "build": "NODE_ENV=production npm run ::server-build && npm run ::client-build",
    "predeploy": "npm run test",
    "deploy": "npm run build && firebase --project=test deploy",
    "postbuild": "node build/post-build",
    "start:server": "npm run ::server-build && firebase serve --only functions",
    "deploy:server": "npm run ::server-build && ./node_modules/.bin/firebase --project=test deploy --only functions",
    "server:shell": "npm run build && firebase functions:shell",
    "server:logs": "firebase functions:log",
    "check-types": "tsc --no-emit",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel test check-types check-format lint build",
    "precommit": "lint-staged",
    "::client-dev": "webpack-dev-server --progress --config ./build/webpack.client.js",
    "::client-build": "NODE_ENV=production webpack -p --config ./build/webpack.client.js",
    "::server-build": "node build/prepare-server && NODE_ENV=production webpack -p --display-error-details --config ./build/webpack.server.js"
  },
  "jest": {
    "roots": [
      "<rootDir>/src",
      "<rootDir>/test"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^client-common(.*)$": "<rootDir>/src/client-common/$1",
      "^shared(.*)$": "<rootDir>/src/shared/$1",
      "^server(.*)$": "<rootDir>/src/server/$1"
    }
  }
}
